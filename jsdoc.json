{
  "extends": [
    "plugin:jsdoc/recommended-typescript-error"
  ],
  "rules": {
    "jsdoc/require-jsdoc": [
      "error",
      {
        "contexts": [
          "TSPropertySignature",
          "TSMethodSignature",
          "TSTypeAliasDeclaration",
          "TSInterfaceDeclaration",
          "TSEnumDeclaration",
          "MethodDefinition",
          "ClassDeclaration",
          "ArrowFunctionExpression",
          "ObjectExpression",
          "PropertyDefinition"
        ],
        "publicOnly": true,
        "checkConstructors": false
      }
    ],
    "jsdoc/require-param": "off",
    "jsdoc/require-returns": "off",
    "jsdoc/tag-lines": "off"
  }
}
